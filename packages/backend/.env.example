# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/claude_code_router?schema=public"

# Redis
REDIS_URL="redis://localhost:6379"

# JWT
JWT_SECRET="your-super-secret-jwt-key-change-in-production"
JWT_ACCESS_EXPIRES_IN="15m"
JWT_REFRESH_EXPIRES_IN="7d"

# Server
PORT=3000
NODE_ENV="development"

# Claude API
CLAUDE_API_KEY="your-claude-api-key"
CLAUDE_API_BASE_URL="https://api.anthropic.com"

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Admin
ADMIN_PASSWORD=your-secure-admin-password

# GitHub OAuth
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret
GITHUB_CALLBACK_URL=http://localhost:3000/api/v1/auth/github/callback
FRONTEND_URL=http://localhost:5173

# Third Party Account Proxy
THIRD_PARTY_PROXY_ENABLED=false
THIRD_PARTY_PROXY_URL=http://127.0.0.1:7890

# Antigravity OAuth
ANTIGRAVITY_CLIENT_ID=your-antigravity-client-id
ANTIGRAVITY_REDIRECT_URI=http://localhost:5173/admin/accounts/antigravity/callback
